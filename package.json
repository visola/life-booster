{
  "name": "life-booster",
  "version": "1.0.0",
  "scripts": {
    "bundle": "rm -Rf build/frontend/static && parcel build -d build/frontend/static --public-url / src/main/frontend/html/index.html",
    "start": "rm -Rf build/frontend/static && parcel watch -d build/frontend/static --public-url / src/main/frontend/html/index.html"
  },
  "dependencies": {
    "@uppy/core": "^1.0.2",
    "@uppy/react": "^1.0.2",
    "@uppy/xhr-upload": "^1.0.2",
    "axios": "^0.21.1",
    "babel-preset-env": "^1.7.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "less": "^3.8.1",
    "parcel-bundler": "^1.12.5"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  }
}
